<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IServiceBasicHttp">
          <security mode="TransportCredentialOnly">
            <transport clientCredentialType="Windows" />
          </security>
        </binding>
        <binding name="CustomBinding_IServiceSoap11">
          <security mode="Transport" />
        </binding>
      </basicHttpBinding>
      <customBinding>
        <binding name="CustomBinding_IServiceSoapMsBin1">
          <binaryMessageEncoding />
          <httpTransport />
        </binding>
      </customBinding>
      <netTcpBinding>
        <binding name="NetTcpBinding_IServiceNetTcp">
          <reliableSession enabled="true" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="http://wcfserviceforloadtest/ServiceBasicHttp.svc"
          binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceBasicHttp"
          contract="ServiceReferenceBasicHttp.IServiceBasicHttp" name="BasicHttpBinding_IServiceBasicHttp" />
      <endpoint address="net.tcp://wcfserviceforloadtest/ServiceNetTcp.svc"
          binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IServiceNetTcp"
          contract="ServiceReferenceNetTcp.IServiceNetTcp" name="NetTcpBinding_IServiceNetTcp">
        <identity>
          <servicePrincipalName value="host/WcfServiceForLoadTest" />
        </identity>
      </endpoint>
      <endpoint address="https://wcfserviceforloadtest/ServiceSoap11.svc"
          binding="basicHttpBinding" bindingConfiguration="CustomBinding_IServiceSoap11"
          contract="ServiceReferenceSoap11.IServiceSoap11" name="CustomBinding_IServiceSoap11" />
      <endpoint address="http://wcfserviceforloadtest/ServiceSoapMsBin1.svc"
          binding="customBinding" bindingConfiguration="CustomBinding_IServiceSoapMsBin1"
          contract="ServiceReferenceSoapMsBin1.IServiceSoapMsBin1" name="CustomBinding_IServiceSoapMsBin1" />
    </client>
  </system.serviceModel>
</configuration>